//Задача 66: Задайте значения M и N. 
//Напишите программу, которая найдёт сумму натуральных элементов в промежутке от M до N.
//M = 1; N = 15 -> 120
//M = 4; N = 8. -> 30

int m = входные числа("Введите M: ");
int n = входные числа("Введите N: ");
int temp = m;

если (m > n)
{
 m = n;
 n = temp;
}

PrintSumm(m, n, temp = 0);

аннулирует результат печати (int m, int n, int summ)
{
 summ = summ + n;
 если (n <= m)
    {
 Консоль.Write($"Сумма элементов = {summ} ");
 Возврат;
    }
 PrintSumm(m, n - 1, summ);
}

int InputNumbers (строковый ввод)
{
 Консоль.Запись (input);
 вывод int = Преобразовать.В INT32(консоль.ReadLine());
 возвращаемый вывод;
}
